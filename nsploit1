local player = game.Players.LocalPlayer
local gui = Instance.new("ScreenGui")
gui.Name = "NSPLO1t"
gui.Parent = player:WaitForChild("PlayerGui")

-- Loading Screen
local loadingFrame = Instance.new("Frame")
loadingFrame.Size = UDim2.new(1, 0, 1, 0)
loadingFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
loadingFrame.Parent = gui

local loadingText = Instance.new("TextLabel")
loadingText.Size = UDim2.new(0.3, 0, 0.1, 0)
loadingText.Position = UDim2.new(0.35, 0, 0.4, 0)
loadingText.Text = "Loading..."
loadingText.TextColor3 = Color3.new(1, 1, 1)
loadingText.TextScaled = true
loadingText.Font = Enum.Font.GothamBold
loadingText.BackgroundTransparency = 1
loadingText.Parent = loadingFrame

local loadingBarBG = Instance.new("Frame")
loadingBarBG.Size = UDim2.new(0.5, 0, 0.05, 0)
loadingBarBG.Position = UDim2.new(0.25, 0, 0.5, 0)
loadingBarBG.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
loadingBarBG.Parent = loadingFrame

local loadingBar = Instance.new("Frame")
loadingBar.Size = UDim2.new(0, 0, 1, 0)
loadingBar.BackgroundColor3 = Color3.fromRGB(0, 0, 128) -- Navy Blue
loadingBar.Parent = loadingBarBG

-- Animate Loading Bar
for i = 1, 100 do
    loadingBar.Size = UDim2.new(i / 100, 0, 1, 0)
    wait(0.02)
end
loadingFrame:Destroy()

-- Main Button
local button = Instance.new("TextButton")
button.Size = UDim2.new(0, 80, 0, 40)
button.Position = UDim2.new(0.1, 0, 0.1, 0)
button.Text = "NS"
button.TextScaled = true
button.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
button.TextColor3 = Color3.new(1, 1, 1)
button.Parent = gui

local buttonCorner = Instance.new("UICorner")
buttonCorner.CornerRadius = UDim.new(0.3, 0)
buttonCorner.Parent = button

-- Main UI
local nsFrame = Instance.new("Frame")
nsFrame.Size = UDim2.new(0.4, 0, 0.4, 0)
nsFrame.Position = UDim2.new(0.3, 0, 0.3, 0)
nsFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
nsFrame.Visible = true
nsFrame.Parent = gui

local nsFrameCorner = Instance.new("UICorner")
nsFrameCorner.CornerRadius = UDim.new(0.1, 0)
nsFrameCorner.Parent = nsFrame

-- Home Tab
local homeTab = Instance.new("Frame")
homeTab.Size = UDim2.new(1, 0, 1, 0)
homeTab.Parent = nsFrame

local nameText = Instance.new("TextLabel")
nameText.Size = UDim2.new(0.5, 0, 0.1, 0)
nameText.Position = UDim2.new(0.02, 0, 0.02, 0)
nameText.Text = "Welcome, " .. player.Name
nameText.TextColor3 = Color3.fromRGB(200, 200, 200)
nameText.TextScaled = true
nameText.Font = Enum.Font.Gotham
nameText.BackgroundTransparency = 1
nameText.Parent = homeTab

local gameText = Instance.new("TextLabel")
gameText.Size = UDim2.new(0.5, 0, 0.1, 0)
gameText.Position = UDim2.new(0.02, 0, 0.12, 0)
gameText.Text = "Game: " .. game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
gameText.TextColor3 = Color3.fromRGB(180, 180, 180)
gameText.TextScaled = true
gameText.Font = Enum.Font.Gotham
gameText.BackgroundTransparency = 1
gameText.Parent = homeTab

-- Stats
local fpsText = Instance.new("TextLabel")
fpsText.Size = UDim2.new(0.3, 0, 0.1, 0)
fpsText.Position = UDim2.new(0.02, 0, 0.25, 0)
fpsText.Text = "FPS: Calculating..."
fpsText.TextColor3 = Color3.fromRGB(255, 255, 255)
fpsText.TextScaled = true
fpsText.Font = Enum.Font.Gotham
fpsText.BackgroundTransparency = 1
fpsText.Parent = homeTab

local pingText = Instance.new("TextLabel")
pingText.Size = UDim2.new(0.3, 0, 0.1, 0)
pingText.Position = UDim2.new(0.02, 0, 0.35, 0)
pingText.Text = "Ping: Calculating..."
pingText.TextColor3 = Color3.fromRGB(255, 255, 255)
pingText.TextScaled = true
pingText.Font = Enum.Font.Gotham
pingText.BackgroundTransparency = 1
pingText.Parent = homeTab

-- TP Tab
local tpTab = Instance.new("Frame")
tpTab.Size = UDim2.new(1, 0, 1, 0)
tpTab.Visible = false
tpTab.Parent = nsFrame

if game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name == "Proxima Hangout" then
    local tpButton = Instance.new("TextButton")
    tpButton.Size = UDim2.new(0.5, 0, 0.1, 0)
    tpButton.Position = UDim2.new(0.25, 0, 0.4, 0)
    tpButton.Text = "Top Cube"
    tpButton.TextScaled = true
    tpButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    tpButton.TextColor3 = Color3.new(1, 1, 1)
    tpButton.Parent = tpTab

    tpButton.MouseButton1Click:Connect(function()
        local destination = game.Workspace:FindFirstChild("TeleportDestination")
        if destination then
            player.Character:SetPrimaryPartCFrame(destination.CFrame)
        end
    end)
end

-- Misc Tab
local miscTab = Instance.new("Frame")
miscTab.Size = UDim2.new(1, 0, 1, 0)
miscTab.Visible = false
miscTab.Parent = nsFrame

local rejoinButton = Instance.new("TextButton")
rejoinButton.Size = UDim2.new(0.5, 0, 0.1, 0)
rejoinButton.Position = UDim2.new(0.25, 0, 0.4, 0)
rejoinButton.Text = "Rejoin"
rejoinButton.TextScaled = true
rejoinButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
rejoinButton.TextColor3 = Color3.new(1, 1, 1)
rejoinButton.Parent = miscTab

rejoinButton.MouseButton1Click:Connect(function()
    player:Kick("Rejoin is in development. For now, we just kick you :D")
end)

-- Tab Switching
button.MouseButton1Click:Connect(function()
    nsFrame.Visible = not nsFrame.Visible
end)

-- FPS & Ping Update
game:GetService("RunService").RenderStepped:Connect(function()
    fpsText.Text = "FPS: " .. math.floor(1 / game:GetService("RunService").RenderStepped:Wait())
    pingText.Text = "Ping: " .. player:GetNetworkPing() * 1000 .. "ms"
end)
