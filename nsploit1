local player = game.Players.LocalPlayer
local gui = Instance.new("ScreenGui")
gui.Name = "NSPLO1t"
gui.Parent = player:WaitForChild("PlayerGui")

-- Main "NS" Button
local button = Instance.new("TextButton")
button.Size = UDim2.new(0, 80, 0, 40)
button.Position = UDim2.new(0.1, 0, 0.1, 0)
button.Text = "NS"
button.TextScaled = true
button.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
button.TextColor3 = Color3.new(1, 1, 1)
button.Parent = gui

-- Create the NS frame
local nsFrame = Instance.new("Frame")
nsFrame.Size = UDim2.new(0.4, 0, 0.6, 0)
nsFrame.Position = UDim2.new(0.3, 0, 0.2, 0)
nsFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
nsFrame.BackgroundTransparency = 0
nsFrame.Visible = false  -- Initially hidden
nsFrame.Parent = gui

-- Add a corner to the frame
local nsFrameCorner = Instance.new("UICorner")
nsFrameCorner.CornerRadius = UDim.new(0.1, 0)
nsFrameCorner.Parent = nsFrame

-- Add version label
local versionText = Instance.new("TextLabel")
versionText.Size = UDim2.new(0.5, 0, 0.07, 0)
versionText.Position = UDim2.new(0.02, 0, 0.01, 0)
versionText.Text = "NS, v0.1âˆž"
versionText.TextColor3 = Color3.fromRGB(200, 200, 200)
versionText.TextScaled = true
versionText.Font = Enum.Font.Gotham
versionText.BackgroundTransparency = 1
versionText.TextXAlignment = Enum.TextXAlignment.Left
versionText.Parent = nsFrame

-- Add Home Text
local homeText = Instance.new("TextLabel")
homeText.Size = UDim2.new(1, 0, 0.6, 0)
homeText.Position = UDim2.new(0, 0, 0.3, 0)
homeText.Text = "NS"
homeText.TextColor3 = Color3.new(1, 1, 1)
homeText.TextScaled = true
homeText.Font = Enum.Font.GothamBold
homeText.BackgroundTransparency = 1
homeText.Parent = nsFrame

-- Create Tab Buttons
local tabNames = {"Home", "Tp", "Misc"}
local tabButtons = {}

for i, tabName in ipairs(tabNames) do
    local tabButton = Instance.new("TextButton")
    tabButton.Size = UDim2.new(0.1, 0, 0.08, 0)
    tabButton.Position = UDim2.new(0.02, 0, 0.2 + (i - 1) * 0.1, 0)
    tabButton.Text = tabName
    tabButton.TextScaled = true
    tabButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    tabButton.TextColor3 = Color3.fromRGB(0, 150, 255)
    tabButton.Parent = nsFrame

    -- Rounded corners for tabs
    local tabCorner = Instance.new("UICorner")
    tabCorner.CornerRadius = UDim.new(0.3, 0)
    tabCorner.Parent = tabButton

    tabButtons[tabName] = tabButton
end

-- Create Tab Content
local homeContent = Instance.new("Frame")
homeContent.Size = UDim2.new(1, 0, 0.6, 0)
homeContent.Position = UDim2.new(0, 0, 0.3, 0)
homeContent.BackgroundTransparency = 1
homeContent.Visible = true  -- Initially visible
homeContent.Parent = nsFrame

local tpContent = Instance.new("Frame")
tpContent.Size = UDim2.new(1, 0, 0.6, 0)
tpContent.Position = UDim2.new(0, 0, 0.3, 0)
tpContent.BackgroundTransparency = 1
tpContent.Visible = false  -- Initially hidden
tpContent.Parent = nsFrame

local miscContent = Instance.new("Frame")
miscContent.Size = UDim2.new(1, 0, 0.6, 0)
miscContent.Position = UDim2.new(0, 0, 0.3, 0)
miscContent.BackgroundTransparency = 1
miscContent.Visible = false  -- Initially hidden
miscContent.Parent = nsFrame

-- Home Content (Ping, FPS, Health)
local pingLabel = Instance.new("TextLabel")
pingLabel.Size = UDim2.new(0.5, 0, 0.07, 0)
pingLabel.Position = UDim2.new(0.02, 0, 0.01, 0)
pingLabel.Text = "Ping: " .. player.NetworkPing
pingLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
pingLabel.TextScaled = true
pingLabel.Font = Enum.Font.Gotham
pingLabel.BackgroundTransparency = 1
pingLabel.Parent = homeContent

local fpsLabel = Instance.new("TextLabel")
fpsLabel.Size = UDim2.new(0.5, 0, 0.07, 0)
fpsLabel.Position = UDim2.new(0.02, 0, 0.08, 0)
fpsLabel.Text = "FPS: 60"  -- Adjust FPS as needed
fpsLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
fpsLabel.TextScaled = true
fpsLabel.Font = Enum.Font.Gotham
fpsLabel.BackgroundTransparency = 1
fpsLabel.Parent = homeContent

local healthLabel = Instance.new("TextLabel")
healthLabel.Size = UDim2.new(0.5, 0, 0.07, 0)
healthLabel.Position = UDim2.new(0.02, 0, 0.15, 0)
healthLabel.Text = "Health: " .. player.Character.Humanoid.Health
healthLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
healthLabel.TextScaled = true
healthLabel.Font = Enum.Font.Gotham
healthLabel.BackgroundTransparency = 1
healthLabel.Parent = homeContent

-- NS Label on Home
local nsLabel = Instance.new("TextLabel")
nsLabel.Size = UDim2.new(0.5, 0, 0.07, 0)
nsLabel.Position = UDim2.new(0.02, 0, 0.22, 0)
nsLabel.Text = "NS"
nsLabel.TextColor3 = Color3.fromRGB(0, 0, 128)
nsLabel.TextScaled = true
nsLabel.Font = Enum.Font.Gotham
nsLabel.BackgroundTransparency = 1
nsLabel.Parent = homeContent

-- TP Content (Proxima Hangout check)
local tpText = Instance.new("TextLabel")
tpText.Size = UDim2.new(1, 0, 0.3, 0)
tpText.Position = UDim2.new(0, 0, 0.1, 0)
tpText.Text = "Coming Soon"
tpText.TextColor3 = Color3.fromRGB(0, 150, 255)
tpText.TextScaled = true
tpText.Font = Enum.Font.GothamBold
tpText.BackgroundTransparency = 1
tpText.Parent = tpContent

-- Rejoin Button in Misc Tab
local rejoinButton = Instance.new("TextButton")
rejoinButton.Size = UDim2.new(0.5, 0, 0.2, 0)
rejoinButton.Position = UDim2.new(0.25, 0, 0.6, 0)
rejoinButton.Text = "Rejoin"
rejoinButton.TextScaled = true
rejoinButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
rejoinButton.TextColor3 = Color3.new(1, 1, 1)
rejoinButton.Visible = false  -- Initially hidden
rejoinButton.Parent = miscContent

-- Rejoin Button Functionality
rejoinButton.MouseButton1Click:Connect(function()
    game:GetService("TeleportService"):Teleport(game.PlaceId, player)
end)

-- Tab Switching Logic
tabButtons["Home"].MouseButton1Click:Connect(function()
    homeContent.Visible = true
    tpContent.Visible = fals
