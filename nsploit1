gui.Name = "NSPLO1t"

-- Instead of setting 'nsFrame.Visible = true', you might want to make it false initially to prevent unintended visibility.
nsFrame.Visible = true

-- 'GetProductInfo()' is being called every time this script runs, which may be inefficient. Consider caching the game name instead.
if game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name == "Blox Fruits" then

-- The dropdown update logic replaces 'MouseButton1Click' handlers multiple times, causing potential memory leaks.
dropdown.MouseButton1Click:Connect(function()
    dropdown.Text = "Loading..."
    local players = game.Players:GetPlayers()
    for _, p in ipairs(players) do
        if p ~= player then
            dropdown.Text = p.Name
            dropdown.MouseButton1Click:Connect(function()
                noclipTo(p)
            end) -- This creates multiple event connections over time.
        end
    end
end)

